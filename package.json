{
  "name": "github_proj",
  "version": "1.0.0",
  "main": "main.js",
  "description": "",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "electron .",
    "build:win": "electron-builder --win",      
    "build:mac": "electron-builder --mac",      
    "build:linux": "electron-builder --linux", 
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.example.githubproj",          
    "productName": "GithubProjApp",           
    "directories": {
      "output": "dist"                      
    },
    "files": [
      "**/*",
      "!dist",
      "!node_modules/.cache"
    ],
    "win": {
      "target": ["nsis"]                      
    },
    "mac": {
      "target": ["dmg"]
    },
    "linux": {
      "target": ["AppImage"]
    }
  },
  "devDependencies": {
    "electron": "^39.2.1",
    "electron-builder": "^26.0.12"
  },
  "keywords": []
}